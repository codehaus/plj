<project>
    <pomVersion>3</pomVersion>
    <artifactId>pl-j-bsfexec</artifactId>
    <name>pl-j-bsfexec</name>
    <groupId>pl-j</groupId>
    <currentVersion>0.0.5</currentVersion>
    <inceptionYear>2004</inceptionYear>
    <logo>/images/logo.gif</logo>
    <shortDescription>BSF executor for PL-J</shortDescription>
    <mailingLists>
        <mailingList>
            <name>${pom.name} Dev List</name>
        </mailingList>
        <mailingList>
            <name>${pom.name} User List</name>
        </mailingList>
    </mailingLists>
    <dependencies>
        <dependency>
            <groupId>bsf</groupId>
            <artifactId>bsf</artifactId>
            <version>2.3.0</version>
            <type>jar</type>
            <properties/>
        </dependency>
        <dependency>
            <groupId>pl-j</groupId>
            <artifactId>pl-j-api</artifactId>
            <version>0.0.4</version>
            <type>jar</type>
            <properties/>
        </dependency>
        <dependency>
            <groupId>avalon-framework</groupId>
            <artifactId>avalon-framework-api</artifactId>
            <version>4.1.5</version>
            <type>jar</type>
            <properties/>
        </dependency>
        <dependency>
            <groupId>pl-j</groupId>
            <artifactId>pl-j-tools</artifactId>
            <version>0.0.4</version>
            <type>jar</type>
            <properties/>
        </dependency>
    </dependencies>
    <build>
        <sourceDirectory>src/java</sourceDirectory>
        <unitTestSourceDirectory>src/test</unitTestSourceDirectory>
        <unitTest>
            <includes>
                <include>**/*Test.java</include>
            </includes>
        </unitTest>
    </build>
    <properties/>
</project>