<?xml version="1.0" encoding="ISO-8859-1"?>
<project>

  <pomVersion>3</pomVersion>
  <id>pl-j-postgres</id>
  <name>pl-j-postgres</name>
  <currentVersion>0.0.1</currentVersion>
  
  <extends>../../project.xml</extends>

  <inceptionYear>2003</inceptionYear>
  <logo>http://forgeahead.hu/images/plpgj-logo.png</logo>

  <description>Maps PostgreSQL types into java types.</description>
  <shortDescription>PostgreSQL typemapper.</shortDescription>

	<dependencies>
		<dependency>
			<id>ant</id>
			<type>jar</type>
			<artifactId>ant</artifactId>
			<version>1.6</version>
		</dependency>

		<dependency>
			<id>avalon-framework</id>
			<type>jar</type>
			<artifactId>avalon-framework</artifactId>
			<version>4.1.5</version>
		</dependency>

		<dependency>
			<id>log4j</id>
			<type>jar</type>
			<artifactId>log4j</artifactId>
			<version>1.2.8</version>
		</dependency>

		<dependency>
			<id>pl-j</id>
			<type>jar</type>
			<artifactId>pl-j-api</artifactId>
			<version>0.0.1</version>
		</dependency>

	</dependencies>

	<build>
		<nagEmailAddress>hornyakl@inf.elte.hu</nagEmailAddress>
		<sourceDirectory>src/java</sourceDirectory>
	</build>

	<reports>
		<report>maven-faq-plugin</report>
	</reports>

</project>
